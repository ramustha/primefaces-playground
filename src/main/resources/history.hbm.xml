<hibernate-mapping>
    <class name="com.adiwisista.primeface.playground.History" table="history">
        <id column="id" name="id" type="long">
            <generator class="increment"/>
        </id>
        <property column="date_time" name="dateTime" not-null="true" type="timestamp"/>
        <property column="account_id" name="accountId" not-null="true" type="long"/>
        <property column="account_ame" length="100" name="accountName" not-null="true" type="string"/>
        <property column="saldo_awal" name="saldoAwal" not-null="true" type="long"/>
        <property column="debet" name="debet" type="long"/>
        <property column="kredit" name="kredit" type="long"/>
        <property column="saldo_Akhir" name="saldoAkhir" not-null="true" type="long"/>

    </class>
</hibernate-mapping>